@model List<Vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>Movies</th>
        <th>Genre</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var m in @Model)
        {
            <tr>
                <td>@Html.ActionLink(@m.Name, "Detail", "Movies", new {Id = m.Id}, null)</td>
                <td>@m.GenreType.Name</td>
        </tr>
        }
    </tbody>
</table>


